.page-header
  h1 New technology

- form_for(technology, {action: path_to.technologies(), method: 'POST', id: "technology_form"}, function (form) {
  != partial('technologies/form.jade', {locals: {form: form, technology: technology}})
  .actions
    != form.submit('Create technology', {class: 'btn'})
    span= ' or '
    != link_to('Cancel', path_to.technologies(), {class: 'btn'})
- });
